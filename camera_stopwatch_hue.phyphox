      <output>thisHue</output>
    </append>
    <append>
      <input keep="true">thisHue</input>
      <output>hue_last</output>
    </append>
    <!-- Translate input values into 1 (for triggering) and 0 (non-triggering) according to user-selected trigger range -->
    <formula formula="min((1-heaviside([4]))*(heaviside([1_]-[3]) * heaviside([2]-[1_])) + (heaviside([4]))*(heaviside([1_]-[2]) + heaviside([3]-[1_])),1)">
      <input>thisHue</input>
      <input keep="true">threshold_high</input>
      <input keep="true">threshold_low</input>
      <input keep="true">outside</input>
      <output>triggering</output>
    </formula>
    <!-- Dermine times when triggering turns on or off -->
    <differentiate>
      <input keep="true">triggering</input>
      <output>triggering_change</output>
    </differentiate>
    <rangefilter>
      <input keep="true">inT</input>
      <input keep="true">triggering_change</input>
      <input as="min" type="value">0.5</input>
      <output append="true">onEvents</output>
    </rangefilter>
    <rangefilter>
      <input>inT</input>
      <input>triggering_change</input>
      <input as="max" type="value">-0.5</input>
      <output append="true">offEvents</output>
    </rangefilter>
    <append>
      <input keep="true">t</input>
      <output>on0</output>
