  <analysis>
    <!-- Keep input data to show in preview graph -->
    <append>
      <input keep="true">inLuma</input>
      <output append="true">amplitude</output>
    </append>
    <append>
      <input keep="true">inT</input>
      <output append="true">t</output>
    </append>
    <append>
      <input keep="true">inLuma</input>
      <input>luma_last</input>
      <output>luma_last</output>
    </append>
    <append>
      <input>luma_last</input>
      <input>inLuma</input>
      <output>thisLuma</output>
    </append>
    <append>
      <input keep="true">thisLuma</input>
      <output>luma_last</output>
    </append>
    <append>
        <input keep="true">t</input>
        <output>t_last</output>
    </append>
    <subtract>
        <input>t_last</input>
        <input type="value">10</input>
        <output>t_limit</output>
    </subtract>
    <rangefilter>
      <input>t</input>
      <input as="min">t_limit</input>
      <input>amplitude</input>
      <output>t</output>
      <output>amplitude</output>
    </rangefilter>
    <!-- Translate input values into 1 (for triggering) and 0 (non-triggering) according to user-selected trigger range -->
    <formula formula="min((1-heaviside([4]))*(heaviside([1_]-[3]) * heaviside([2]-[1_])) + (heaviside([4]))*(heaviside([1_]-[2]) + heaviside([3]-[1_])),1)">
      <input>thisLuma</input>
      <input keep="true">threshold_high</input>
      <input keep="true">threshold_low</input>
      <input keep="true">outside</input>
      <output>triggering</output>
    </formula>
    <!-- Dermine times when triggering turns on or off -->
    <differentiate>
      <input keep="true">triggering</input>
      <output>triggering_change</output>
    </differentiate>
    <rangefilter>
      <input keep="true">inT</input>
      <input keep="true">triggering_change</input>
      <input as="min" type="value">0.5</input>
      <output append="true">onEvents</output>
    </rangefilter>
    <rangefilter>
      <input>inT</input>
      <input>triggering_change</input>
      <input as="max" type="value">-0.5</input>
      <output append="true">offEvents</output>
    </rangefilter>
    <append>
      <input keep="true">t</input>
      <output>on0</output>
    </append>
    <append>
      <input keep="true">t</input>
      <output>on1</output>
    </append>
    <append>
      <input keep="true">t</input>
      <output>on2</output>
    </append>
    <append>
      <input keep="true">t</input>
      <output>on3</output>
    </append>
    <append>
      <input keep="true">t</input>
      <output>on4</output>
    </append>
    <append>
      <input keep="true">t</input>
      <output>on5</output>
    </append>
    <append>
      <input keep="true">t</input>
      <output>off0</output>
    </append>
    <append>
      <input keep="true">t</input>
      <output>off1</output>
    </append>
    <append>
      <input keep="true">t</input>
      <output>off2</output>
    </append>
    <append>
      <input keep="true">t</input>
      <output>off3</output>
    </append>
    <append>
      <input keep="true">t</input>
      <output>off4</output>
    </append>
    <append>
      <input keep="true">t</input>
      <output>off5</output>
    </append>
    <if greater="true">
      <input clear="false">reset</input>
      <input type="value">0</input>
      <input type="empty"/>
      <output>amplitude</output>
    </if>
    <if greater="true">
      <input clear="false">reset</input>
      <input type="value">0</input>
      <input type="empty"/>
      <output>t</output>
    </if>
    <if greater="true">
      <input clear="false">reset</input>
      <input type="value">0</input>
      <input type="empty"/>
      <output>luma_last</output>
    </if>
    <if greater="true">
      <input clear="false">reset</input>
      <input type="value">0</input>
      <input type="empty"/>
      <output>onEvents</output>
    </if>
    <if greater="true">
      <input clear="false">reset</input>
      <input type="value">0</input>
      <input type="empty"/>
      <output>offEvents</output>
    </if>
    <if greater="true">
      <input clear="false">reset</input>
      <input type="value">0</input>
      <input type="empty"/>
      <output>inT</output>
    </if>
    <if greater="true">
      <input clear="false">reset</input>
      <input type="value">0</input>
      <input type="empty"/>
      <output>inLuma</output>
    </if>
    <append>
      <input type="value">0</input>
      <output>reset</output>
    </append>
